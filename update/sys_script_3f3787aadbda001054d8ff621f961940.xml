        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var createFormAccessRec = new GlideRecord ('x_naci_family_coho_fcsms_form_access');
	createFormAccessRec.initialize();
	
	createFormAccessRec.form_id = current.sys_id;
	createFormAccessRec.form_type = 'fhq';
	createFormAccessRec.email = current.initial_proband.email_address;
	createFormAccessRec.user = getUserID(current.initial_proband.email_address);
	
	createFormAccessRec.insert();
	
	
	function getUserID(email){
		
		var getUser = new GlideRecord ('sys_user');
		getUser.addQuery('email', email);
		getUser.query();
		
		if (getUser.next()){
			return getUser.sys_id;
		}
	}

})(current, previous);]]></script>
