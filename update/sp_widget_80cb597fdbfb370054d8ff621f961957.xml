        <client_script><![CDATA[function($scope,$location, spModal) {
  /* widget controller */
  var c = this;
	var s = $location.search();
	
	if(s.xpage){
		if(s.xpage == 'review_intake_form'){
			c.type = 'review_intake';
		}
		else if(s.xpage == 'review_s_consent'){
			c.type = 'review_s_consent';
		}
		else if(s.xpage == 'review_fhq'){
			c.type = 'review_fhq';
		}
		else{
			c.type = 'review_iiq';
		}
	}
	else{
		c.type = 'review_iiq';
	}
	
	c.goBack = function(){
		s.id = null;
		$location.search(s).replace();
	}
}]]></client_script>
