        <client_script><![CDATA[function($location) {
  /* widget controller */
  var c = this;
	
	var s = $location.search();
	var start_interval = true;
	
	console.log(c.data.type);
	
	if(c.data.type == 'video'){
		$("#image_area").hide();
		$("#slideshow_video > div:gt(0)").hide();
	
		setInterval(function() {
			if(start_interval){
				$('#slideshow_video > div:first')
				.fadeOut(1500)
				.next()
				.fadeIn(1500)
				.end()
				.appendTo('#slideshow_video');
			}
		},  5000);
	}
	else{
		$("#video_area").hide();
		$("#slideshow > div:gt(0)").hide();
	
		setInterval(function() {
			if(start_interval){
				$('#slideshow > div:first')
				.fadeOut(1500)
				.next()
				.fadeIn(1500)
				.end()
				.appendTo('#slideshow');
			}
		},  5000);
	}
	
	
	
	
	c.gotoSlide = function(idx){
		start_interval = false;
		var obj = $('#slideshow > div:first');
		var curr_pos = 1;
		var id = obj.attr('id');
		if(id == 'image-1'){
			curr_pos = 1;
		}
		else if(id == 'image-2'){
			curr_pos = 2;
		}
		else{
			curr_pos = 3;
		}
		var moves = idx - curr_pos;
		if(moves < 0){
			moves = moves + 3;
		}
		if(moves == 1){
			obj.fadeOut();
			obj.appendTo('#slideshow');
			obj = $('#slideshow > div:first');
			obj.fadeIn();
		}
		else if(moves == 2){
			obj.fadeOut();
			obj.appendTo('#slideshow');
			obj = $('#slideshow > div:first');
			obj.fadeOut();
			obj.appendTo('#slideshow');
			obj = $('#slideshow > div:first');
			obj.fadeIn();
		}
		start_interval = true;
	}
}]]></client_script>
