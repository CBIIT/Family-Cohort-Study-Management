        <client_script><![CDATA[function($scope,spModal, $rootScope, $location) {
  var c = this;
  var s = $location.search();

	if (s.user_type && s.user_type == 'e_user') {
			c.user_type = "e_user";
			c.xpage = "p_board";
	}
	else if(s.user_type && s.user_type == 'i_user'){
		c.user_type = "i_user";
		if(s.xpage){
			c.xpage = s.xpage;
		}
		else{
			c.xpage = 'board';
		}
	}
	else{
		c.user_type = "home";
		c.xpage = s.xpage;
	}
	
	c.gotoIntakeForm = function(){
		s.xpage = 'intake';
		//console.log(s);
    $location.search(s).replace();
	}
	
}]]></client_script>
